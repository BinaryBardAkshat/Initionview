<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_enabled="true" android:state_pressed="true">
        <set>
            <objectAnimator android:interpolator="@android:interpolator/linear" android:duration="@integer/button_pressed_animation_duration" android:valueTo="-5dp" android:valueType="floatType" android:propertyName="translationZ"/>
        </set>
    </item>
    <item android:state_enabled="true">
        <set>
            <objectAnimator android:interpolator="@android:interpolator/linear" android:duration="@integer/button_pressed_animation_duration" android:valueTo="0" android:valueType="floatType" android:propertyName="translationZ" android:startDelay="@integer/button_pressed_animation_delay"/>
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:duration="0" android:valueTo="0" android:valueType="floatType" android:propertyName="translationZ"/>
        </set>
    </item>
</selector>
